package com.main
 
import com.app.rwm.model.RunnerData;
import com.app.rwm.enums.PHYSICAL_FITNESS;

rule "physical fitness"
	agenda-group "cooper"
	when
		$r : RunnerData(distance >= 1500, physicalFitness == PHYSICAL_FITNESS.NA)
	then
		System.out.println("hello");
		$r.setPhysicalFitness(PHYSICAL_FITNESS.GOOD);
		update($r);
	end
		